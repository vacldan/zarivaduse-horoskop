# pyswisseph editorconfig file
# see https://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
# max line length should be ~80, no hard limit
max_line_length = off
trim_trailing_whitespace = true

[Makefile]
indent_style = tab

